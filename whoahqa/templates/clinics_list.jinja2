{% extends "base.jinja2" %}
{% block title %}{{ gettext("All Clinics") }}{% endblock %}
{% block page_title %} {{ self.title() }} {% endblock %}
{% block breadcrumbs %}
  <li>
      <i class="icon-angle-right"></i>
  </li>
  <li class="active"> {{ gettext("All Clinics") }} </li>
{% endblock %}
{% block content %}
  <div class="row">
  <div style="margin-left:0px; margin-bottom:5px;" class="row">
        <div class="col-md-2 form-group">
           <input id="search_term" name="search_term" type="search" class="form-control light-table-filter" placeholder="{{ gettext('Search clinics')}}..." data-table="clinics-table"/>
        </div>
  </div>
    <div class="col-md-12">
      <div class="clinics_table">
        {% include '_clinics_table.jinja2' %}
      </div>
    </div>
  </div>
{% endblock content %}
{% block page_scripts %}
  <script src="{{ request.static_url('whoahqa:static/scripts/custom.js') }}"></script>
{% endblock %}
{% block jquery_ready %}
  {{ super() }}
{% endblock %}
