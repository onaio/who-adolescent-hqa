{% extends "base.jinja2" %}
{% block title %}{{ gettext("All Clinics") }}{% endblock %}
{% block page_title %} {{ self.title() }} {% endblock %}
{% block breadcrumbs %}
  <li>
    <i class="icon-home"></i>
    {{ gettext("Clinics") }}
  </li>
  <li><a href="#"> </a></li>
{% endblock %}
{% block content %}
  <div class="row">
    <div class="col-md-12">
      <div class="clinics_table">
        {% include '_clinics_table.jinja2' %}
      </div>
    </div>
  </div>
{% endblock content %}
{% block page_scripts %}
