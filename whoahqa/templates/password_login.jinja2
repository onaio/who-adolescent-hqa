<body class="login">
<!-- BEGIN LOGO -->
<div class="logo">

</div>
<!-- END LOGO -->
<!-- BEGIN LOGIN -->
<div class="content">
  <!-- BEGIN LOGIN FORM -->
  <form class="login-form" action="{{ request.route_url('auth', action='sign-in') }}" method="post">
    <input type="hidden" name="csrf_token" value="{{ request.session.get_csrf_token() }}" />
    <h3 class="form-title">Login to your account</h3>

    {% include "_alerts.jinja2" %}

    <div class="alert alert-error hide">
      <button class="close" data-dismiss="alert"></button>
      <span>Enter any username and password.</span>
    </div>
    <div class="form-group">
      <!--ie8, ie9 does not support html5 placeholder, so we just show field title for that-->
      <label class="control-label visible-ie8 visible-ie9">Username</label>

      <div class="input-icon">
        <i class="icon-user"></i>
        <input class="form-control placeholder-no-fix" type="text"
               autocomplete="off" placeholder="Username" name="username"/>
      </div>
    </div>
    <div class="form-group">
      <label class="control-label visible-ie8 visible-ie9">Password</label>

      <div class="input-icon">
        <i class="icon-lock"></i>
        <input class="form-control placeholder-no-fix" type="password"
               autocomplete="off" placeholder="Password" name="password"/>
      </div>
    </div>
    <div class="form-actions">
    <p class="ona-login">
      <a href="{{ request.route_url('auth', action='login')}}" id="register-btn">
        {{ gettext('Click Here') }}
      </a>
      {{ gettext("To log in as ONA user") }}
    </p>
      <input type="submit" class="btn btn-info pull-right" value="Login">
    </div>
  </form>
  <!-- END LOGIN FORM -->
</div>
<!-- END LOGIN -->
