{% extends "base.jinja2" %}
{% block title %}Unassigned Clinics{% endblock %}
{% block page_title %} {{ self.title() }} {% endblock %}
{% block breadcrumbs %}
  <li>{{ self.page_title() }}</li>
  <li><a href="#"> </a></li>
{% endblock %}
{% block content %}
  <div class="row">
    <div class="col-md-12">
      <form method="post" action="{{ request.route_url('clinics', traverse=('assign')) }}">
        <div class="table-toolbar">
          <div class="btn-group">
            <button type="submit" class="btn btn-primary">
              Assign Selected to Myself <i class="icon-share-alt"></i>
            </button>
          </div>
        </div>
        {% include '_clinics_table.jinja2' %}
      </form>
    </div>
  </div>
{% endblock content %}
{% block page_scripts %}
  <script src="{{ request.static_url('whoahqa:static/scripts/custom.js') }}"></script>
{% endblock %}
{% block jquery_ready %}
  {{ super() }}
  Custom.addCheckAllListener();
{% endblock %}