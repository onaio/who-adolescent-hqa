#~ msgstr "Adolescente Usuário"

#~ msgid "Health-care Provider"
#~ msgstr "Profissional de Saúde"

#~ msgid "Support Staff"
#~ msgstr "Equipe de Apoio"

#~ msgid "Health Facility Manager"
#~ msgstr "Gestor da Unidade de Saúde"

#~ msgid "Outreach Worker"
#~ msgstr "Agentes Comunitários"

#~ msgid "Community Member"
#~ msgstr "Membro da Comunidade"

